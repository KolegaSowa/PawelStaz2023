<!--liquibase formatted xml-->
<!--changeset pawel:1-->
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet author="pawel" id="1">
        <createTable tableName="EMPLOYEE">
            <column name="id_employee" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column name="name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="last_name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="id_role" type="BIGINT">
                <constraints nullable="false" foreignKeyName="fk_id_role" references="ROLE(id_role)"/>
            </column>
            <column name="id_employee_data" type="BIGINT">
                <constraints nullable="false" foreignKeyName="fk_id_employee_data" references="EMPLOYEE_DATA(id_employee_data)"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>